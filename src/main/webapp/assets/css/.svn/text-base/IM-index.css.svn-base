.im-table a{
margin: 0 5px;
    padding: 5px 8px;
    border-radius: 5px;	margin: 0 5px;
	color: white;
}
.im-table a:nth-of-type(1){
	    background: #1a6edc;
}
.im-table a:nth-of-type(2){
	    background:#e63f4a;
}
.im-table tbody tr{
	border-bottom: 2px solid #f0f0f0;
	border-radius: 2.5px;

}

.im-table th{
	height: 37px;
	line-height: 37px;
	text-align: center;
	background: #daeaff;
	color:#323843;
