html,body{
	height:100%;
}

body{
/*  background:#f4f9ff; */
background:#fff;
}

.layui-body{
 overflow: hidden;
}

.layui-body::-webkit-scrollbar {
	display: none ; 
}

.layui-layout-body .layui-header>ul{
  background-image:url(../img/index_01.jpg) !important;
  background-size:cover !important;
  background-position:0 0 !important;
  height: 60px;
 
}

.layui-nav-tree .layui-nav-child dd.layui-this, .layui-nav-tree .layui-nav-child dd.layui-this a, .layui-nav-tree .layui-this, .layui-nav-tree .layui-this>a, .layui-nav-tree .layui-this>a:hover{
	background-color: #2b6dc1 !important;
	color: #fff;
}

.layui-nav-tree .layui-nav-item a:hover{
  background:#5793df;
  
}
.layui-nav-tree{
	/* border-right:25px solid #EBEDF0; */
	height:100%;
}
.layui-nav-itemed .layui-nav-child{
  background-color:#FCFCFC !important;
}

.layui-layout-body .layui-nav{
  background:#FCFCFC;
}
.layui-layout-body .layui-side{
  background:#FCFCFC;
  width: 250px;
  top:60px;
}
.layui-layout-body .layui-side-scroll,.layui-nav-tree{
  width: 100%
}
	

.layui-layout-admin .layui-body{
  top: 50px;
  left: 240px;
  height:100%;
}

.cont_layout{
  height:100%
}

.layui-nav .layui-nav-item{
  line-height:50px;
  
}

.layui-row{
  height: 34px;
  margin-top: 11px;
}
.cont_bar{
  margin-top: 11px;
  margin-right: 33px;
  display: flex;
  height: 34px;
  line-height: 34px;
  justify-content:flex-end;
  border-bottom: 2px solid #e6e8eb;
}
.cont_bar p:nth-of-type(2){
	background:url('../img/个人中心.png') no-repeat 2px 2px;
	padding-left:20px;
	padding-right:5px;
	border:1px solid #ccc;
	border-radius:3px;
	height: 25px;
    line-height: 25px;
    margin-top: 3px;
    cursor: pointer;
}
.cont_bar p:nth-of-type(2):hover{
	background:url('../img/grzx2.png') no-repeat 2px 2px;
	border:1px solid #4898FF;
	color:#4898FF;
}
.cont_bar p:nth-of-type(1){
	float: right;
	margin-right: 50px;
}
.cont_bar_icon{
  display: block;
  height: 34px;
  width: 16px;
  background: url("./images/user.png") no-repeat center;
}
.layui-nav .layui-nav-item a.header_end{
  display: inline-block;
  background:#E6424C url("../img/end.png") no-repeat 55px center; 
  color:#fff;
  border-radius:20px;
  height:30px;
  width:80px;
  vertical-align:middle;
  padding:0;
  line-height:30px;
  padding-left: 20px;
  box-sizing:border-box;
}
.layui-nav-tree .layui-nav-bar{
  width: 0
}
.layui-nav-bar{
  width: 0 !important;
}

iframe{
  width: 100%;
  height:100%;
}
