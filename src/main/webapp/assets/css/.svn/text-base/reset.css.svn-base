html,body,div,span,
applet,object,iframe,
h1,h2,h3,h4,h5,h6,p,blockquote,pre,
a,abbr,acronym,address,big,cite,code,
del,dfn,em,font,img,ins,kbd,q,s,samp,
small,strike,strong,sub,sup,tt,var,
dd,dl,dt,li,ol,ul,
fieldset,form,label,legend,
table,caption,tbody,tfoot,thead,tr,th,td,input{
	margin: 0;
	padding: 0;
	outline: none;
	font-size: 14px;
	font-family: Microsoft YaHei;
}

body{
	/* background: #f4f9ff; */
}
table {
	border-collapse: collapse;
	border-spacing: 0;
	width:100%;
}
ol,ul {
	list-style: none;
}
q:before,q:after,
blockquote:before,blockquote:after {
	content: "";
}
img{
	vertical-align: middle;
}
a{
  text-decoration: none;
  cursor: pointer;
}

select{
	color:#4f6aa8;
}
.usename{
	font-size:12px;
	color: #4f6aa8;
}
.hea{
		height: 50px;
		line-height: 50px;
		text-align: center;
		font-size: 16px;
		color: #fff;
		background: #73b0ff;
	}
	.to_top{
	display:inline-block;
	position:fixed;
	bottom:5%;
	right:10%;
	width:49px;
	height:67px;
	cursor:pointer;
	background:url("../img/top.gif")
}
.icon{
	display:inline-block;
		width:10px;
	height:10px;
	position:absolute;
	top:0;
	bottom:0;
	margin:auto;	
}
.fl{
	float:left;
}
.fr{
	float:right;
}
.clear:after{
    clear:both;
    height:0;
    content:"";
    display: block;
    visibility: hidden;
}
.clear{
	zoom:1;/*zoom是IE专有属性，可解决ie6,ie7浮动问题，IE8以上和非IE浏览器才支持伪类:after。*/
}
body{
	padding-bottom:95px
}